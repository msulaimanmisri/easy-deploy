{
    "name": "sulaimanmisri/easy-deploy",
    "description": "A simple Laravel package to simplify deployment commands",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Muhamad Sulaiman Misri",
            "homepage": "https://sulaimanmisri.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "SulaimanMisri\\EasyDeploy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SulaimanMisri\\EasyDeploy\\EasyDeployServiceProvider"
            ]
        }
    }
}